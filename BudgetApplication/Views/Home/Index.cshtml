@model BudgetApplication.Models.Tesseract;
@{
    ViewData["Title"] = "Budget Application";
}

<div class="jumbotron">
    <h1>Budget Application</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <h2></h2>
    </div>
</div>


    <h2>Processed Image:</h2>
    @{
        Tesseract tess = new Tesseract("IMG_1.jpg");
        string text = tess.getText();
        IEnumerable<string> data = tess.seperateStringToArrayByWhiteSpaces(text);
    }
@text <br />
    @foreach (var item in data)
    {
        <tr>
            <td>
                <p> @item </p>
            </td>
        </tr>
    }
